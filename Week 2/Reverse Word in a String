class Solution(object):
    def reverseWords(self, s):
        """
        :type s: str
        :rtype: str
        """
        output = ""
        lst = s.split()
        for i in range(len(lst)-1, -1, -1): #iterating lst backwards 
            output += lst[i]
            if i > 0:
                output += ' '
        return output
